{
  "name": "tile-catalog",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "start": "node ./node_modules/vite/bin/vite.js preview --port $PORT --host 0.0.0.0",
    "render-build": "npm ci && npm run build"
  },
  "dependencies": {
    "vite": "^6.0.0",
    "terser": "^5.36.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}